<div class="container">
    <h1>Business Contacts List</h1>
  <div class="row">
      <div class="col">
         <table class="table table-striped table-bordered table-hover">
          <thead class="table-dark">
              <tr>
                 <th scope="col" class="text-center"></th>
                 <th scope="col">Contact Name</th>
                 <th scope="col">Contact Number</th>
                 <th scope="col">Email Address</th>
               
                 <th></th>
                 <th></th>
              </tr>
          </thead>
          <tbody id="movieList">
            <% for(let count = 0; count < users.length; count++) {%>
             
              <tr>
                <th scope="row" class="text-center"><%= count + 1%></th>
                <td><%= movies[count].Name %></td>
                <td><%= movies[count].Year %></td>
                <td><%= movies[count].Director %></td>
                <td><%= movies[count].Rating %></td>
                <td></td>
                <td></td>
              </tr>

             <% } %>
            
    </tbody>
   </table>
          
  
      </div>
  </div>
  
  </div>
  